# MelodyVerse Environment Variables

# API Keys
REACT_APP_PINATA_API_KEY=your_pinata_api_key_here
REACT_APP_PINATA_API_SECRET=your_pinata_api_secret_here

# Solana Network Configuration
REACT_APP_SOLANA_NETWORK=devnet  # Options: devnet, testnet, mainnet-beta
REACT_APP_SOLANA_RPC_ENDPOINT=https://api.devnet.solana.com

# Storage Configuration
REACT_APP_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# Application Settings
REACT_APP_MAX_MUSIC_DURATION=120  # Maximum music duration (seconds)
REACT_APP_DEFAULT_MUSIC_STYLE=modern  # Default music style

# Analytics Service
REACT_APP_ANALYTICS_ID=your_analytics_id 